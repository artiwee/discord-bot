{
  "name": "ghom-prettify",
  "version": "3.0.0",
  "description": "Prettier extension for cast language to parsers and parse.",
  "main": "dist/index.js",
  "types": "dist",
  "scripts": {
    "test": "tsc && node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "format": "prettier --write package.json tsconfig.json src tests",
    "build": "tsc",
    "prepublishOnly": "npm run test && npm run format"
  },
  "prettier": {
    "semi": false
  },
  "keywords": [
    "prettier",
    "discord",
    "markdown"
  ],
  "author": "ghom",
  "license": "ISC",
  "dependencies": {
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/prettier": "^2.7.3",
    "jest": "^29.7.0",
    "typescript": "^5.4.5"
  },
  "optionalDependencies": {
    "@prettier/plugin-php": "^0.22.2",
    "@prettier/plugin-ruby": "^4.0.4",
    "@prettier/plugin-xml": "^3.4.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "testMatch": [
      "**/tests/*"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
